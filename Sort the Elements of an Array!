#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int stu[n];
    for(int i=0;i<n;i++){
        scanf("%d",&stu[i]);
    }
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-i-1;j++){
        if(stu[j]>stu[j+1]){
            int temp=stu[j];
            stu[j]=stu[j+1];
            stu[j+1]=temp;
          }
        }
    }
    for(int i=0;i<n;i++){
        printf("%d ",stu[i]);
    }
    return 0;
}
