#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char S[1001];
    scanf("%[^\n]", S); // Read input string
    int i, j, flag;

    for(i = 0; S[i] != '\0'; i++) {
        flag = 0;
        for(j = 0; j < i; j++) {
            if(S[i] == S[j]) {
                flag = 1;
                break;
            }
        }
        if(flag == 0) {
            printf("%c", S[i]);
        }
    }
    printf("\n");
    return 0;
}
