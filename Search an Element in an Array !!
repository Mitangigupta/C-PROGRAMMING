#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int size;
    scanf("%d",&size);
    int stu[size];
    for(int i=0;i<size;i++){
        scanf("%d",&stu[i]);
    }
    int key,flag=0;
    scanf("%d",&key);
    int i;
    for(i=0;i<size;i++){
       if(stu[i]==key){
           flag=1;
               
           break;
       }
    }
    if(flag==1)
             printf("Element found at position %d",i+1);
    else
        printf("Element not found ");
    return 0;
}
