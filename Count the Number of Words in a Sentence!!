#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char S[2005];
    fgets(S, sizeof(S), stdin);
    int count = 0;
    int inWord = 0;
    for (int i = 0; S[i] != '\0'; i++) {
        if (isalpha(S[i])) {
            if (!inWord) {
                count++;
                inWord = 1;
            }
        } else {
        
            inWord = 0;
        }
    }

    printf("%d\n", count);
    return 0;
}
