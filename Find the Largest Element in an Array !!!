#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i;
    scanf("%d",&n);
    int stu[n];
    for(i=0;i<n;i++){
    scanf("%d",&stu[i]);
    }
    int max=stu[0];
     for(i=1;i<n;i++){
         
     if(stu[i]>max)
         max=stu[i];
     }
     printf("%d",max);
    return 0;
}
