#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>
int main() {
    char str[1000];
    int count =0;
    fgets(str,sizeof(str),stdin);
    for(int i=0;str[i]!='\0';i++){
        char ch = tolower(str[i]);
        if(ch=='a'||ch=='e'||ch=='o'||ch=='u'||ch=='i'){
            count++;
        }
    }
    printf("%d\n",count);
    return 0;
}
