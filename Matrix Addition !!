#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int n,i,j,n1;
    scanf("%d%d",&n,&n1);
    int stu[n][n1];
    for(i=0;i<n;i++){
        for(int j=0;j<n1;j++){
    scanf("%d",&stu[i][j]);
        }
    }
    int stu1[n][n1];
    for(i=0;i<n;i++){
        for(int j=0;j<n1;j++){
        scanf("%d",&stu1[i][j]);
        }
    }
     int sum[n][n1];
     for(i=0;i<n;i++){
        for(int j=0;j<n1;j++){
          sum[i][j]=stu[i][j]+stu1[i][j];
        }
     }
     for(i=0;i<n;i++){
        for(int j=0;j<n1;j++){
            printf("%d ",sum[i][j]);
        }
         printf("\n");
     }
    return 0;
