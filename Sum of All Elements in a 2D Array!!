#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,n1,i,sum=0,j;
    scanf("%d%d",&n,&n1);
    int stu[n][n1];
    for(i=0;i<n;i++){
        for(j=0;j<n1;j++){
            scanf("%d",&stu[i][j]);
        }
    }
            //sum=0;
    for(i=0;i<n;i++){
        
    for(j=0;j<n1;j++){
        sum=sum+stu[i][j];
        }
    }
    printf("%d\n",sum);
    return 0;
}
