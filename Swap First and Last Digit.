#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,first,last,c=0;
    scanf("%d",&num);
    if(num>=0 && num<=9){
        first=last=num;
        printf("%d\n",num);
    }else{
        last=num%10;
        num=num/10;
        int count = 0,temp = num;
        while(temp != 0){
            temp=temp/10;
            count++;
        }
        
        c=count-1;
        first= num/pow(10,c);
        num=num%(int)pow(10,c);;
        num=last*pow(10,count)+num*10+first;
        printf("%d\n",num);
    }
    return 0;
}
   
