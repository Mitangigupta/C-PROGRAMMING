#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i;
    scanf("%d",&n);
    int a[n];
    int b[n];
    for(i=0;i<n;i++){
        scanf("%d",a+i);
    }
    for(i=0;i<n;i++){
        scanf("%d",b+i);
    }
    for(i=0;i<n;i++){
        int temp=*(a+i);
        *(a+i)=*(b+i);
        *(b+i)=temp;
    }
    for(i=0;i<n;i++){
    printf("%d ",*(a+i));
    }
    printf("\n");
    for(i=0;i<n;i++){
    printf("%d ",*(b+i));
    }
    return 0;
}
