#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int u;
   float b,c;
   scanf("%d",&u);
   scanf("%f",&b);

   if(u<=50){
       b=u*0.50;
   }else if(u>=51 && u<=150){
        b=50*0.5+(u-50)*0.75;
   }
    else if(u>=151 && u<=250){
        b=50*0.5+100*0.75+(u-150)*1.20;
    }
    else{
        b=50*0.5+100*0.75+100*1.20+(u-250)*1.50;
    }
    b=b+(b*0.20);
    printf("%.2f",b);
    return 0;
}
