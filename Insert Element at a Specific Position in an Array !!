#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int stu[n+1];
    for(int i=0;i<n;i++){
        scanf("%d",&stu[i]);
    }
    int x,pos;
    scanf("%d%d",&x,&pos);
    for(int i=n;i>=pos;i--){
    stu[i]=stu[i-1];//left ki taraf jaega suppose[5]10,20,30,40,50,50-10,20,30,40,40,50
    }
    stu[pos-1]=x;
    for(int i=0;i<=n;i++){
        printf("%d ",stu[i]);
    }
    return 0;
}
