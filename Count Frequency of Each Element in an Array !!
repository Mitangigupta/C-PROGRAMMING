#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int n;
    scanf("%d",&n);
    int stu[n];
    int count1[n];
    for(int i=0;i<n;i++){
    scanf("%d",&stu[i]);
    count1[i]=0;
    }   
    for(int i=0;i<n;i++){
    if(count1[i]==0){
    int count=1;
    for(int j=i+1;j<n;j++){
         if(stu[i]==stu[j]){
             count++;
             count1[j]=1;
    }
    }
    printf("%d: %d\n",stu[i],count);
    }
    }
    return 0;
}
