#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int size;
    scanf("%d",&size);
    int stu[size];
    for(int i=0;i<size;i++){
        scanf("%d",&stu[i]);
    }
    int key,flag=0;
    scanf("%d",&key);
    int i;
    int count=0;
    for(i=0;i<size;i++){
        if(stu[i]==key)
            count++;
    }
    printf("%d",count);
    return 0;
}
