#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    char str[1001];
    int alphabets = 0, digits = 0, special = 0;

    fgets(str, sizeof(str), stdin);

    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];

        if (isalpha(ch)) {
            alphabets++;
        }
        else if (isdigit(ch)) {
            digits++;
        }
        else if (ch != '\n') {   
            special++;
        }
    }

    printf("%d %d %d\n", alphabets, digits, special);

    
    return 0;
}
