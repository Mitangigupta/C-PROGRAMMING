#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    scanf("%c",&ch);
    if((ch==65)||(ch==69)||(ch==73)||(ch==79)||(ch==85||ch==97)||(ch==101)||(ch==105)||(ch==111)||(ch==117)){
        printf("Vowel");
    }
    else if((ch>=66 && ch<=68)||(ch>=70 && ch<=72)||(ch>=78)||(ch>=80 && ch<=84)||(ch>=86 && ch<=90)||(ch>=98 && ch<=100)||(ch>=102 && ch<=104)||(ch>=106 && ch<=110)||(ch>=112 && ch<=116)){
        printf("Consonant");
    }
    else{
        printf("Invalid Input");
    }
    return 0;
}
